{
  "version": 2,
  "builds": [
    {
      "src": "server/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "5mb",
        "pythonVersion": "3.9"
      }
    }
  ],
"regions":["bom1"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.py"
    }
  ]
}